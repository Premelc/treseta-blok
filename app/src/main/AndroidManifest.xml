<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="com.google.android.gms.permission.AD_ID"/>

    <application
        android:name="com.premelc.tresetacounter.MainApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/treseta_launcher"
        android:label="@string/app_title"
        android:supportsRtl="true"
        android:theme="@style/Theme.TresetaCounter"
        tools:targetApi="34">
        <activity
            android:name="com.premelc.tresetacounter.MainActivity"
            android:hardwareAccelerated="true"
            android:exported="true"
            android:screenOrientation="nosensor"
            android:theme="@style/Theme.TresetaCounter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9680718533779299~1710027674" />
    </application>
</manifest>